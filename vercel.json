{
  "version": 2,
  "routes": [
    {
      "src": "^/\\.well-known/farcaster\\.json$",
      "status": 307,
      "headers": {
        "Location": "https://api.farcaster.xyz/miniapps/hosted-manifest/019b2767-ab13-0d94-cf04-9e116545a1aa"
      }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        { "key": "Content-Type", "value": "application/manifest+json" }
      ]
    }
  ]
}
